{% extends 'admin/base.html.twig' %}

{% block title %}タグ詳細 | {{ admin_name }}{% endblock %}


{% block breadcrumbs %}
    <li class="breadcrumb-item active">タグ詳細</li>
 {% endblock %}
{% block body %}
    <h1>タグ詳細</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ tag.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ tag.name }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ tag.createdAt ? tag.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ tag.updatedAt ? tag.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_stag_index') }}">back to list</a>

    <a href="{{ path('admin_tag_edit', {'id': tag.id}) }}">edit</a>

    {{ include('admin/tag/_delete_form.html.twig') }}
{% endblock %}
